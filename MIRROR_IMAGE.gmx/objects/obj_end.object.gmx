        ds_list_add(ListOfArrays, ini_read_real("Save", string(num),0));
        --num;   
    }
    for (var num2 = 0; num2 &lt; ds_list_size(ListOfArrays); ++num2)
    {
        if (ListOfArrays[| num2] &lt; playerScore)
